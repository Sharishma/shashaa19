import java.util.*;
class Solution
{
public int FirstMissingPositive(int[] nums)
{
int n=nums.length;
int i=0;
while(i<n)
{
if(nums[i]>0 && nums[i]<=n && nums[i]!=nums[nums]-1])
{
int temp=nums[i];
nums[i]=nums[nums[i]-1];
nums[temp-1]=temp;
}
else
{
i++;
}
}
for (i=0,i<n;i++)
{
i(nums[i]!=i+1)
return(i+1);
}
return n+1;
}
}


Output:
[1,2,0]
3
