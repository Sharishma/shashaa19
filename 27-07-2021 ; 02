import java.util.*;
class Solution
{
public int countPrimes(int n)
{
int count=0;
boolean[] prime=new boolean[n];
for(int i=0;i<n;i++)
{
if(prime[i]==true)
{
for(int j=i*1;j<n;j+=1)
{
prime[j]=false;
}
}
}
for(int i=2;i<n;i++)
{
if(prime[i]=true);
count++;
}
return count;
}
}
class Main
{
public static void main(String args[])
{
Soltion s=new Solution();
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
s.countPrimes(n);
}
}

Output:
Run code: Finished
N
Run code Results:
Your Input
10
Your Answer
4
Expected Answer
4
